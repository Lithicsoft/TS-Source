<Page x:Class="Lithicsoft_Trainer_Studio.CSharp.IC.ModelTester"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Lithicsoft_Trainer_Studio.CSharp.IC"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="655"
      Title="ModelTester" Loaded="Page_Loaded">

    <Grid>
        <Image x:Name="image1" HorizontalAlignment="Left" Height="329" Margin="10,10,0,0" VerticalAlignment="Top" Width="329" OpacityMask="Black" Stretch="Fill"/>
        <Button x:Name="button1" Content="Choose Image" HorizontalAlignment="Left" Margin="10,350,0,0" Height="30" VerticalAlignment="Top" Foreground="White" BorderBrush="White" Background="#FF5D6B99" Click="Button_Click" IsEnabled="False"/>
        <Label x:Name="label1" Content="Predict: None" HorizontalAlignment="Left" Margin="352,10,0,0" VerticalAlignment="Top" Foreground="#FF0078D7"/>

    </Grid>
</Page>
